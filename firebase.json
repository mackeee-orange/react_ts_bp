{
  "hosting": [
    {
      "target": "prd",
      "public": "dummy_public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "app"
        }
      ]
    }
  ],
  "functions": {
    "source": "functions",
    "runtime": "nodejs12",
    "region": "asia-northeast1"
  }
}
