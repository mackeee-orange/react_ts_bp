    "start:staging": "env-cmd -f .env.staging node scripts/start.js",
    "start:production": "env-cmd -f .env.production node scripts/start.js",
    "stylelint": "stylelint --cache --color --fix src/**/*.scss",
    "test": "node scripts/test.js"
  },
  "importSort": {
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "eslint"
    }
  },
